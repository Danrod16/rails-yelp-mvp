<h1>Restaurants#index</h1>
<p>Find me in app/views/restaurants/index.html.erb</p>

<% @restaurants.each do |restaurant| %>
  <h3><%= link_to restaurant.name, restaurant_path(restaurant) %></h3>
  <h5><%= restaurant.category %> </h5>
<% end %>

<h3><%= link_to '+Add a new Restaurant!', new_restaurant_path %></h3>
